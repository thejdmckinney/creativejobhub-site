{
  "buildCommand": "echo 'No build needed - static site'",
  "outputDirectory": ".",
  "framework": null,
  "redirects": [
    {
      "source": "https://www.creativejobhub.com/:path*",
      "destination": "https://creativejobhub.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}
